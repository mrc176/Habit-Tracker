// Author: Caitlyn Barrow

public class endOfFile {

    int numFailures;
    int numSuccesses;
    int failAndSucc;
    double succDecimal;
    int successPercentage;

    public int getNumFailures() {
        return numFailures;
    }
    public void setNumFailures(int numFail) {
        numFailures = numFail;
    }

    public int getNumSuccesses() {
        return numSuccesses;
    }
    public void setNumSuccesses(int numSucc) {
        numSuccesses = numSucc;
    }

    public int averagePercent(int numFailures int numSuccesses) {
        numFailures + numSuccesses = failAndSucc;
        numSuccesses / failAndSucc = succDecimal;
        succDecimal * 100 = successPercentage;
        return successPercentage;
    }
}